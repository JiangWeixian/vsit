{
  "name": "vit",
  "version": "0.0.0",
  "packageManager": "pnpm@7.33.0",
  "description": "todo",
  "keywords": ["vit"],
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://github.com/vit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vit.git",
    "directory": "packages/vit"
  },
  "bugs": {
    "url": "https://github.com/vit/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "files": [
    "dts",
    "dist",
    "dist-client"
  ],
  "bin": {
    "vit": "bin/index.mjs"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "types": "./dts/exports/browser.d.ts",
        "require": "./dist/browser.cjs",
        "import": "./dist/browser.mjs"
      },
      "node": {
        "types": "./dts/exports/node.d.ts",
        "require": "./dist/node.cjs",
        "import": "./dist/node.mjs"
      }
    }
  },
  "main": "dist/node.cjs",
  "module": "dist/node.mjs",
  "types": "dts/exports/node.d.ts",
  "scripts": {
    "copy-client": "esno ./scripts/client.ts",
    "build:dts": "rimraf dts && ttsc",
    "build:lib": "rimraf dist && rollup -c --silent --environment BUILD:production",
    "build": "run-s build:*",
    "dev": "rollup -c -w",
    "preview": "node ./bin/index.mjs",
    "vitest": "vitest",
    "test": "pnpm run build && pnpm run vitest",
    "prepublishOnly": "publish-police",
    "ci:publish": "pnpm run build && pnpm changeset publish",
    "ci:version": "pnpm changeset version",
    "ci:snapshot": "pnpm changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpm changeset publish --tag beta"
  },
  "dependencies": {
    "sirv": "^2.0.3",
    "vite": "^4.3.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@rollup/plugin-alias": "4.0.3",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.1.3",
    "@types/node": "20.3.1",
    "@types/rimraf": "^3.0.2",
    "body-parser": "1.20.2",
    "cac": "6.7.12",
    "consola": "^2.15.3",
    "console-feed": "3.3.0",
    "cz-emoji": "^1.3.1",
    "debug": "^4.3.4",
    "esbuild": "^0.18.6",
    "esno": "^0.16.3",
    "execa": "^6.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "inquirer": "8.2.0",
    "npm-run-all": "^4.1.5",
    "ofetch": "^1.0.1",
    "ora": "6.0.1",
    "picocolors": "1.0.0",
    "publish-police": "^0.1.0",
    "rimraf": "^3.0.2",
    "rollup": "3.19.1",
    "rollup-plugin-condition-exports": "2.0.0-next.3",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-node-externals": "5.1.2",
    "rollup-plugin-size": "^0.3.1",
    "source-map-support": "^0.5.21",
    "ttypescript": "^1.5.15",
    "typescript": "^4.6.4",
    "typescript-transform-paths": "^3.4.6",
    "ufo": "^1.1.2",
    "vitest": "^0.22.1"
  }
}
