  "scripts": {
    "test": "vitest",
    "dev:cli": "pnpm --filter=./packages/vit dev",
    "dev": "vite",
    "build:cli": "pnpm --filter=./packages/vit build",
    "build": "tsc && vite build",
    "preview": "vite preview",
